  "tokenColors": [
    {
      "settings": {
        "foreground": "#bbb"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment", "unused.comment", "wildcard.comment"],
      "settings": {
        "foreground": "#64727d"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#eabe9a"
      }
    },
    {
      "name": "Entity",
      "scope": "entity",
      "settings": {
        "foreground": "#F7E3AF"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#bbb"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#b18bb1"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#D0D6B5",
        "fontStyle": "regular"
      }
    },
    {
      "name": "Meta",
      "scope": "meta",
      "settings": {
        "foreground": "#DB6E8F"
      }
    },
    {
